{
  "applicationId": "org.longui.twrppccontrol",
  "version": "1.3.0 beta",
  "defaultMode": "window",
  "documentRoot": "/react-src/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory",
    "os.spawnProcess",
    "os.writeProcessInput",
    "os.killProcess",
    "os.getSpawnedProcesses",
    "os.getEnvs",
    "os.execCommand"
  ],
  "modes": {
    "window": {
      "title": "TWRP-PC-Control",
      "width": 400,
      "height": 750,
      "minWidth": 400,
      "minHeight": 700,
      "icon": "/react-src/src/assets/download.png",
      "enableInspector": false,
      "useSavedState": false
    }
  },
  "cli": {
    "binaryName": "TWRP-PC-Control_[1.3.0]",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/react-src/build/",
    "frontendLibrary": {
      "patchFile": "/react-src/index.html",
      "devUrl": "http://localhost:3000",
      "projectPath": "/react-src/",
      "initCommand": "pnpm install",
      "devCommand": "pnpm run dev",
      "buildCommand": "pnpm run build",
      "waitTimeout": 20000
    },
    "binaryVersion": "6.3.0"
  },
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  }
}
